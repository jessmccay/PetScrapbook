<Page loaded="onLoaded" xmlns="http://schemas.nativescript.org/tns.xsd">
  <StackLayout>
    <ListView items="{{ pages }}" itemTap="onItemTap">
      <ListView.itemTemplate> 
        <StackLayout>
          <Label text="{{ title, title + ' Scrapbook Page' }}" />
        </StackLayout>
      </ListView.itemTemplate>
    </ListView>
  </StackLayout>
</Page>
